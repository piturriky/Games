<html>
	<head>
		<title>{{ title}}</title>
	</head>
	<body>
		<h1>All the {{ title }}:</h1>
		{% if vars %}
		<ul>
		{% for var in vars %}
		{% if var.name %}
		<li>
		<A HREF="http://localhost:8000/{{link}}/{{var.name}}">{{var.name}}</A>
		</li>
		{% else %}
		<li>
		<A HREF="http://localhost:8000/{{link}}/{{var.username}}">{{var.username}}</A>
		</li>
		{% endif %}
		{% endfor %}
		</ul>
		{% else %}
		<p>Doesn't exist {{ title}}.</p>
		{% endif %}
	</body>
</html>
